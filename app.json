{
  "expo": {
    "name": "SpadesScore",
    "slug": "SpadesScore",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffe3d3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.leku.spades",
      "supportsTablet": true
    },
    "scheme": "spades",
    "android": {
      "package": "com.leku.spades",
      "versionCode": 1,
      "permissions": []
    }
  }
}
